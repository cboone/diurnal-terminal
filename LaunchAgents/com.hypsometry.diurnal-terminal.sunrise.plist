<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.hpm.sunrise</string>

    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>0</integer>
    </dict>

    <key>Program</key>
    <string>$(brew --prefix)/bin/diurnal-terminal sunrise</string>

    <key>StandardOutPath</key>
    <string>/var/log/com.hpm.sunrise.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/com.hpm.sunrise.log</string>
  </dict>
</plist>
